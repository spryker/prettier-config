{
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "endOfLine": "lf",
    "plugins": ["prettier-plugin-twig-melody"],
    "twigPrintWidth": 120,
    "twigFollowOfficialCodingStandards": false,
    "twigAlwaysBreakObjects": false
}
